])\n","\n","    # Prepare the data for plotting\n","    max_activations = df_grouped.xs('max', level=1, axis=1)\n","    min_activations = df_grouped.xs('min', level=1, axis=1)\n","    mean_activations = df_grouped.xs('mean', level=1, axis=1)\n","    std_activations = df_grouped.xs('std', level=1, axis=1)\n","\n","    # Calculate mean of mean activations and mean of std activations\n","    mean_of_means = mean_activations.mean(axis=1)\n","    mean_of_stds = std_activations.mean(axis=1)\n","\n","    # Get unique n_layer values\n","    unique_layers = sorted(df_acts['n_layer'].unique())\n","\n","    # Prepare data for boxplot\n","    data = [df_acts[df_acts['n_layer'] == layer].iloc[:, 1:4096].values.flatten() for layer in unique_layers]\n","\n","    # Plotting the distributions and boxplots\n","    fig, ax1 = plt.subplots(figsize=(14, 8))\n","\n","    # Plot the line plots with correct x values\n","    x_values = np.arange(len(unique_layers))\n","    ax1.plot(x_values, max_activations.mean(axis=1), label='Max Activation')\n","    ax1.plot(x_values, min_activations.mean(axis=1), label='Min Activation')\n","    ax1.plot(x_values, mean_of_means, label='Mean Activation')\n","    ax1.plot(x_values, mean_of_means + mean_of_stds, label='Mean + STD Activation')\n","    ax1.plot(x_values, mean_of_means - mean_of_stds, label='Mean - STD Activation')\n","\n","    ax1.set_xlabel('Layer Number (n_layer)')\n","    ax1.set_ylabel('Activation Value')\n","    ax1.set_title('Activation Distributions by Layer')\n","    ax1.legend()\n","\n","    # Plotting the boxplot on the same figure\n","    ax1.boxplot(data, showfliers=showfliers, positions=x_values, widths=0.5)\n","\n","    ax1.set_xticks(ticks=x_values)\n","    ax1.set_xticklabels(unique_layers)\n","\n","    plt.show()\n","\n","plot_boxplots_with_distributions()\n","\n"],"metadata":{"id":"MNiELK1615PM","colab":{"base_uri":"https://localhost:8080/","height":625},"executionInfo":{"status":"ok","timestamp":1743960659532,"user_tz":-120